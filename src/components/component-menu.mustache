<div class="component-menu" data-view="Menu" data-collection="Posts">
    <h1>Data coming from CMS</h1>
    <div class="pure-g">
        <script type="text/x-handlebars-template" id="component-post">
            <% data.map(element => { %>
                <div class="pure-u-1-3">
                    <div class="post">
                        <div class="title">
                            <%= element.title %>
                        </div>
                        <p class="body">
                            <%= element.body %>
                        </p>
                    </div>
                </div>
            <% }) %>
        </script>
    </div>
</div>

